<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="william.eshop.mapper.order.OrderStatusMapper">
    <!--结果集映射-->
    <resultMap id="BaseResultMap" type="william.eshop.model.order.OrderStatus">
        <id column="order_id" property="orderId"/>
        <result column="order_status" property="orderStatus"/>
        <result column="created_time" property="createdTime"/>
        <result column="pay_time" property="payTime"/>
        <result column="deliver_time" property="deliverTime"/>
        <result column="success_time" property="successTime"/>
        <result column="close_time" property="closeTime"/>
        <result column="comment_time" property="commentTime"/>
    </resultMap>

    <sql id="BaseColumn">
        order_id, order_status, created_time, pay_time, deliver_time, success_time, close_time, comment_time
    </sql>

</mapper>